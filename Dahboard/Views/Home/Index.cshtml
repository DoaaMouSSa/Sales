
@{
    ViewData["Title"] = "Home Page";
}
@{Layout = null;}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Home</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="LoginRoot/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="LoginRoot/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="LoginRoot/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="LoginRoot/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="LoginRoot/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="LoginRoot/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="LoginRoot/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="LoginRoot/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="LoginRoot/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="LoginRoot/css/util.css">
    <link rel="stylesheet" type="text/css" href="LoginRoot/css/main.css">
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
                <form class="login100-form validate-form" onsubmit="return false">
                    <span class="login100-form-title p-b-49">
                        تسجيل الدخول
                    </span>
                    <div class="alert alert-danger alert-dismissible fade show" style="display:none;" id="alertLoginFail" role="alert">
                        <strong>خطأ فى اسم المستخدم أو كلمه المرور</strong>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="alert alert-success" style="display:none;" id="alertLoginSuccess" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <strong>تم تسجيل الدخول بنجاح</strong>
                    </div>
                   
                    <div class="wrap-input100 validate-input m-b-23" data-validate="Username is reauired">
                        <p class="label-input100">اسم المستخدم</p>
                        <input class="input100" type="text" name="username" id="user_name" placeholder="اسم المستخدم" autocomplete="off" onkeyup="RemoveAlertForWrongData();ClearWarningMsgs();">
                    </div>
                    <p class="text-danger" id="RequiredUserName"></p>
                    <div class="wrap-input100 validate-input" data-validate="Password is required">
                        <p class="label-input100">كلمة السر</p>
                        <input class="input100" type="password" name="pass" id="password" placeholder="كلمة السر" autocomplete="off" onkeyup="RemoveAlertForWrongData();ClearWarningMsgs();">
                    </div>
                    <p class="text-danger" id="RequiredPassword"></p>
                    <div class="wrap-input100" data-validate="Password is required">
                        <p class="label-input100">الشركه</p>
                        <input class="input100" autocomplete="off">
                    </div>
                    <div class="wrap-input100" data-validate="Password is required">
                        <p class="label-input100">القسم</p>
                        <input class="input100" autocomplete="off">
                    </div>
                    <div class="wrap-input100" data-validate="Password is required">
                        <p class="label-input100">الفرع الادارى</p>
                        <input class="input100" autocomplete="off">
                    </div>
                    <div class="wrap-input100" data-validate="Password is required">
                        <p class="label-input100">نوع النشاط</p>
                        <input class="input100" autocomplete="off">
                    </div>

                    <br>
                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button class="login100-form-btn text-bold" onclick="Login();">
                                تسجيل الدخول
                            </button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>


    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    <script src="LoginRoot/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="LoginRoot/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="LoginRoot/vendor/bootstrap/js/popper.js"></script>
    <script src="LoginRoot/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="LoginRoot/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="LoginRoot/vendor/daterangepicker/moment.min.js"></script>
    <script src="LoginRoot/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="LoginRoot/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="LoginRoot/js/main.js"></script>
    <script src="LoginRoot/js/Login.js"></script>
    <script src="~/js/common/ClearValidateWarning.js"></script>
</body>
</html>